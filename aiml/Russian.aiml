<aiml>
<category><pattern>ПРИВЕТ</pattern><template>Привет</template></category>
  <category>
    <pattern> МОЕ ИМЯ * </pattern>
    <template>
      Привет!<think>
        <set name="name_r">
          <star/>
        </set>
      </think>
    </template>
  </category>

  <category>
    <pattern>ПОКА</pattern>
    <template>Прощай, <get name="name_r"></get></template>
  </category>

  <category>
    <pattern> МЕНЯ ЗОВУТ * </pattern>
    <template><srai>МОЕ ИМЯ <star/></srai></template>
  </category>
</aiml>
